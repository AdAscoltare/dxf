{
  "name": "dxf",
  "version": "3.3.1",
  "description": "DXF parser for node/browser",
  "main": "lib/index.js",
  "bin": {
    "dxf-to-svg": "lib/cli.js"
  },
  "engines": {
    "node": ">=6.9.0"
  },
  "scripts": {
    "lint": "standard",
    "test:unit": "mocha --require babel-core/register --recursive test/unit/**",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:functional:watch": "npm run unit -- --watch",
    "test": "npm run lint && npm run test:unit && npm run test:functional",
    "compile": "babel --optional runtime -d lib/ src/",
    "dist": "mkdir -p dist && browserify lib/index.js --standalone dxf -o dist/dxf.js",
    "prepublishOnly": "rimraf dist/ lib/ && npm run compile && npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bjnortier/dxf.git"
  },
  "keywords": [
    "DXF",
    "Parser",
    "SVG"
  ],
  "author": "Ben Nortier <ben@bjnortier.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bjnortier/dxf/issues"
  },
  "homepage": "https://github.com/bjnortier/dxf",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "browserify": "^16.2.2",
    "expect": "^23.6.0",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.2",
    "standard": "^12.0.1"
  },
  "dependencies": {
    "commander": "^2.17.1",
    "lodash.clonedeep": "^4.5.0",
    "pretty-data": "^0.40.0",
    "round10": "^1.0.3",
    "vecks": "^3.7.2"
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "before",
      "beforeEach",
      "after"
    ],
    "ignore": [
      "lib/",
      "dist/"
    ]
  }
}
